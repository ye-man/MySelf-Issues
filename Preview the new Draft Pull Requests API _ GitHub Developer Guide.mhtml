.highlight-headers + pre { border-radius: 0px 0px 3px 3px; margin-top: 0px; border-top-color: rgb(221, 221, 221); }

.highlight-headers + pre.highlight { margin-top: -2px; }

.highlight-headers + :not(pre) { margin-top: 24px; }

.command-line { background-color: rgb(68, 68, 68); color: rgb(255, 255, 255); border-radius: 3px; border: none; position: relative; }

.command-line em { color: rgb(249, 254, 100); }

.command-line span.comment { color: rgb(204, 204, 204); }

.command-line span.output { color: rgb(99, 228, 99); }

.highlight-graphql .k { color: rgb(167, 29, 93); }

.highlight-graphql .n { color: rgb(237, 106, 67); }

.highlight-graphql .err { background-color: transparent; }

#staging-header { line-height: 34px; color: rgba(255, 255, 255, 0.5); background-color: rgb(34, 34, 34); }

#staging-header p { color: rgb(255, 255, 255); font-size: 12px; padding-left: 20px; margin: 0px; }

.integrations-2-marker { padding-left: 10px; }

.deprecation-notice { border-left: 2px solid rgb(232, 64, 13); background: rgb(253, 242, 236); }

.preview-notice { border-left: 2px solid rgb(151, 193, 218); background-color: rgb(216, 235, 248); color: rgb(38, 76, 114); }

.github-apps-marker { padding-bottom: 5px; padding-left: 5px; }

.deprecation-note, .deprecation-warning { padding: 15px; font-size: 12px; text-align: left; }

.deprecation-warning { max-width: 960px; margin-top: 30px; color: rgb(97, 58, 0); border: 1px solid rgb(220, 168, 116); border-radius: 3px; background: -webkit-linear-gradient(top, rgb(255, 227, 200), rgb(245, 218, 192)); box-shadow: rgba(0, 0, 0, 0.1) 0px 1px 3px; text-shadow: rgba(255, 255, 255, 0.8) 0px 1px 0px; }

.deprecation-warning + .alert { margin-top: 5px; }

.deprecation-warning > :first-child { margin-top: 0px; }

.deprecation-warning + #bootcamp { margin-top: 20px; }

.deprecation-note { max-width: 960px; margin-top: 30px; color: rgb(156, 36, 0); border: 1px solid rgb(218, 151, 151); border-radius: 3px; background: -webkit-linear-gradient(top, rgb(248, 216, 216), rgb(239, 208, 208)); box-shadow: rgba(0, 0, 0, 0.1) 0px 1px 3px; text-shadow: rgba(255, 255, 255, 0.8) 0px 1px 0px; }

.deprecation-note + .alert { margin-top: 5px; }

.deprecation-note > :first-child { margin-top: 0px; }

.ajax_paginate, .deprecation-note + #bootcamp, .paginate-wrap { margin-top: 20px; }

.deprecation-overview-layout-padding { padding-top: 30px; }

@media only screen and (-webkit-min-device-pixel-ratio: 2), only screen and (min-resolution: 192dpi) {
  #searchfield { background-image: url("/assets/images/search@2x.png"); background-size: 13px 13px; }
  .cancel-search { background-image: url("/assets/images/cancel@2x.png"); background-size: 14px 14px; }
  .sidebar-module .arrow-btn { background-image: url("/assets/images/expand-arrows@2x.png"); background-size: 73px 80px; }
  .api-status .good, .api-status .major, .api-status .minor, .api-status .unknown { background-size: 32px 32px; }
  .api-status .unknown { background-image: url("/assets/images/status-icon-unknown@2x.png"); }
  .api-status .good { background-image: url("/assets/images/status-icon-good@2x.png"); }
  .api-status .minor { background-image: url("/assets/images/status-icon-minor@2x.png"); }
  .api-status .major { background-image: url("/assets/images/status-icon-major@2x.png"); }
  .rss-subscribe { background-image: url("/assets/images/feed-icon@2x.png"); background-size: 28px 28px; }
}

.pagination a, .pagination em, .pagination > span { position: relative; margin-left: -1px; font-size: 13px; font-style: normal; padding: 7px 12px; color: rgb(64, 120, 192); vertical-align: middle; cursor: pointer; background: rgb(255, 255, 255); border: 1px solid rgb(229, 229, 229); user-select: none; }

code .c, code .c1, code .cm, code .cs, code .ge { font-style: italic; }

.pagination a:first-child, .pagination em:first-child, .pagination > span:first-child { margin-left: 0px; border-top-left-radius: 3px; border-bottom-left-radius: 3px; }

.pagination a:last-child, .pagination em:last-child, .pagination > span:last-child { border-top-right-radius: 3px; border-bottom-right-radius: 3px; }

.pagination a:focus, .pagination a:hover, .pagination em:focus, .pagination em:hover, .pagination > span:focus, .pagination > span:hover { z-index: 2; background-color: rgb(231, 231, 231); border-color: rgb(229, 229, 229); text-decoration: none; }

.pagination .selected { z-index: 3; }

.pagination .current, .pagination .current:hover { z-index: 3; color: rgb(255, 255, 255); background-color: rgb(64, 120, 192); border-color: rgb(64, 120, 192); }

.pagination .disabled, .pagination .disabled:hover, .pagination .gap, .pagination .gap:hover { background-color: rgb(250, 250, 250); cursor: default; color: rgb(211, 211, 211); }

.pagination .octicon { width: 8px; text-align: center; }

.ajax_paginate { display: block; }

.ajax_paginate a { float: none; display: block; padding: 6px; text-align: center; }

.ajax_paginate.loading a { text-indent: -3000px; background-color: rgb(234, 234, 234); background-image: url("/images/spinners/octocat-spinner-16px-EAF2F5.gif"); background-repeat: no-repeat; background-position: center center; border-color: rgb(197, 197, 197); }

@media screen and (-webkit-min-device-pixel-ratio: 2), not all {
